<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date %level [%thread] %logger{10} [%file:%line] [%X{transactionId}] %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="com.davidmaisuradze.gymapplication" level="INFO"/>
    <root level="error">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
